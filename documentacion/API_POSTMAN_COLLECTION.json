      "item": [
        {
          "name": "Autenticación",
          "item": [
            {
              "name": "Login Datero",
              "request": {
                "method": "POST",
                "header": [
                  {
                    "key": "Content-Type",
                    "value": "application/json"
                  }
                ],
                "body": {
                  "mode": "raw",
                  "raw": "{\n    \"email\": \"datero@example.com\",\n    \"password\": \"password123\"\n}"
                },
                "url": {
                  "raw": "{{base_url}}/datero/auth/login",
                  "host": ["{{base_url}}"],
                  "path": ["datero", "auth", "login"]
                }
              },
              "event": [
                {
                  "listen": "test",
                  "script": {
                    "exec": [
                      "if (pm.response.code === 200) {",
                      "    var jsonData = pm.response.json();",
                      "    pm.collectionVariables.set('datero_token', jsonData.data.token);",
                      "}"
                    ]
                  }
                }
              ]
            },
            {
              "name": "Obtener Usuario (Me)",
              "request": {
                "method": "GET",
                "header": [
                  {
                    "key": "Authorization",
                    "value": "Bearer {{datero_token}}"
                  }
                ],
                "url": {
                  "raw": "{{base_url}}/datero/auth/me",
                  "host": ["{{base_url}}"],
                  "path": ["datero", "auth", "me"]
                }
              }
            },
            {
              "name": "Refrescar Token",
              "request": {
                "method": "POST",
                "header": [
                  {
                    "key": "Authorization",
                    "value": "Bearer {{datero_token}}"
                  }
                ],
                "url": {
                  "raw": "{{base_url}}/datero/auth/refresh",
                  "host": ["{{base_url}}"],
                  "path": ["datero", "auth", "refresh"]
                }
